@using DVSRegister.CommonUtility.Models.Enums;
@using DVSRegister.Extensions
@model RegisterPublishLogsViewModel
@{
    ViewData["Title"] = "Updates : Register of digital identity and attribute services";
    Layout = "~/Views/Shared/_Layout_Register.cshtml";
    ViewData["ActiveUpdates"] = "govuk-header__navigation-item--active";
}
<div class="govuk-width-container">
    @Html.ActionLink("Back", "Register", "Register", null, new { @class = "govuk-back-link" })
    <main class="govuk-main-wrapper">
        <div class="govuk-grid-row">
            <div class="govuk-grid-column-two-thirds">
                <h1 class="govuk-heading-xl">Updates : Register of digital identity and attribute services</h1>
                @Html.Partial("~/Views/Register/PartialViews/_NotificationBanner.cshtml")
                @foreach (var item in Model.RegisterPublishLog)
                {
                    <h2 class="govuk-heading-m">@DateTimeExtensions.FormatDateTime(@item.CreatedTime)</h2>
                    <p class="govuk-body">@item.Description</p>
                }

            </div>
        </div>
    </main>
</div>

